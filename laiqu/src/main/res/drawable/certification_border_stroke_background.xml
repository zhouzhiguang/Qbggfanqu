<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <stroke
                android:width="@dimen/dimen_6px"
                android:color="@color/dinner_recyclerview_attention_color"
                android:dashGap="@dimen/dimen_20px"
                android:dashWidth="@dimen/dimen_23px" />

            <solid android:color="@color/white" />

            <corners android:radius="5dp" />
        </shape>
    </item>

</layer-list>